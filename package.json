  "description": "A Node.js tool to fetch event data from the RA.co GraphQL API and save it as a JSON file",
  "main": "eventFetcher.js",
  "scripts": {
    "start": "node eventFetcher.js",
    "server": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop raco-graphql-client",
    "pm2:restart": "pm2 restart raco-graphql-client",
    "pm2:delete": "pm2 delete raco-graphql-client",
    "pm2:logs": "pm2 logs raco-graphql-client",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["resident-advisor", "events", "scraper", "graphql"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "node-cron": "^3.0.3",
    "dotenv": "^16.3.1",
    "pushover-notifications": "^1.2.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
